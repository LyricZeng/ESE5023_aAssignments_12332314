    "#4.Add or double\n",
    "import random\n",
    "def Least_moves(x):  \n",
    "   if x == 1:  \n",
    "       return 0  \n",
    "   elif x % 2 == 0:  \n",
    "       return 1 + Least_moves(x // 2)  \n",
    "   else:  \n",
    "       return 1 + Least_moves((x - 1) // 2)\n",
    "x = random.randint(0, 100)  \n",
    "print(Least_moves(x))  "
   ]
  }
